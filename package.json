{
  "name": "airbnb-cloudflare-backend",
  "version": "0.0.0",
  "private": true,
  "main": "src/index.ts",
  "scripts": {
    "deploy": "vercel",
    "test": "vitest",
    "build": "tsc ",
    "start": "node src/index.ts",
    "db:generate": "drizzle-kit generate:pg --schema ./src/db/schema.ts --out=./drizzle/migrations",
    "db:migrate": "tsx ./drizzle/migrate.ts",
    "db:seed": "tsx ./drizzle/seed.ts",
    "db:studio": "drizzle-kit studio --port 3000 --verbose",
    "email:dev": "email dev"
  },
  "dependencies": {
    "@kinde-oss/kinde-nodejs-sdk": "^1.2.3",
    "@neondatabase/serverless": "^0.9.0",
    "@react-email/components": "0.0.15",
    "drizzle-orm": "^0.30.3",
    "drizzle-zod": "^0.5.1",
    "hono": "^4.1.2",
    "newrelic": "^11.15.0",
    "react-email": "2.1.0",
    "resend": "^3.2.0"
  },
  "devDependencies": {
    "@cloudflare/vitest-pool-workers": "^0.1.0",
    "@cloudflare/workers-types": "^4.20240314.0",
    "dotenv": "^16.4.5",
    "drizzle-kit": "^0.20.14",
    "pg": "^8.11.3",
    "postgres": "^3.4.3",
    "tsx": "^4.7.1",
    "typescript": "^5.0.4",
    "vitest": "1.3.0",
    "wrangler": "^3.0.0"
  }
}
